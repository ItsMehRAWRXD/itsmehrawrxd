{
  "name": "RawrZ Security Platform - Production Deployment",
  "version": "2.0.0",
  "description": "Complete security platform with real black hat capabilities",
  "main": "api-server-real.js",
  "scripts": {
    "start": "node api-server-real.js",
    "dev": "nodemon api-server-real.js",
    "test": "node test-all-features.js",
    "build": "docker build -t rawrz-platform .",
    "deploy": "docker run -d -p 3000:3000 --name rawrz-platform rawrz-platform"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "crypto": "^1.0.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "child_process": "^1.0.2",
    "util": "^0.12.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "features": {
    "real_encryption": true,
    "dual_encryption": true,
    "roslyn_compilation": true,
    "native_cpp_compilation": true,
    "java_compilation": true,
    "python_compilation": true,
    "rust_compilation": true,
    "go_compilation": true,
    "black_hat_capabilities": true,
    "anti_debug": true,
    "anti_vm": true,
    "anti_sandbox": true,
    "timing_evasion": true,
    "hardware_fingerprinting": true,
    "process_hollowing": true,
    "dll_injection": true,
    "credential_harvesting": true,
    "persistence": true,
    "lateral_movement": true,
    "network_scanning": true,
    "irc_bot": true,
    "stealth_generation": true,
    "polymorphic_code": true,
    "anti_analysis": true,
    "virus_scanning": true,
    "memory_management": true,
    "performance_optimization": true
  },
  "deployment": {
    "local": {
      "port": 3000,
      "host": "localhost",
      "ssl": false
    },
    "droplet": {
      "port": 3000,
      "host": "0.0.0.0",
      "ssl": true,
      "domain": "rawrz-platform.com"
    },
    "docker": {
      "image": "rawrz-platform",
      "ports": ["3000:3000"],
      "volumes": ["./processed:/app/processed"],
      "environment": ["NODE_ENV=production"]
    }
  },
  "security": {
    "real_implementations_only": true,
    "no_simulations": true,
    "production_ready": true,
    "black_hat_capabilities": true,
    "advanced_evasion": true,
    "credential_harvesting": true,
    "persistence_mechanisms": true,
    "lateral_movement": true
  }
}
